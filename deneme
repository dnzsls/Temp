        print(f"   Ort. Fark (vs raw): {q_data['diff_vs_raw'].mean():.2f}")
        print(f"   Ort. Fark (vs need): {q_data['diff_vs_need'].mean():.2f}")
        print(f"   Ort. GerÃ§ekleÅŸen SL: %{q_data['achieved_sl'].mean():.1f}")


